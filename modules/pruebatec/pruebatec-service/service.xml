<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="com.sinensia.pruebatec">
	<namespace>ricoh</namespace>
	<entity local-service="true" name="UserPrueba" remote-service="true" uuid="true">
		<column name="userId" primary="true" type="long" />
		<column name="name" type="String" />
		<column name="familyName" type="String" />
		<column name="birthDate" type="Date" />
		<column name="email" type="String" />
		<column name="regDate" type="Date" />
	</entity>
</service-builder>